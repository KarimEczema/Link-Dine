{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    { "src": "/$", "dest": "/api/index.php" },
    { "src": "/acceuil", "dest": "/api/accueil.php" },
    { "src": "/emplois", "dest": "/api/emplois.php" },
    { "src": "/images/(.*)", "dest": "/public/images/$1" },
    { "src": "/css/(.*)", "dest": "/public/css/$1" }
  ]
}