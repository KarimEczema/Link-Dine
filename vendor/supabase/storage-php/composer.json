{
    "name": "supabase/storage-php",
    "type": "library",
    "description": "Supabase Storage for PHP",
    "keywords": ["supabase","storage", "storage-php"],
    "homepage": "https://github.com/supabase-community/storage-php",
    "autoload": {
        "psr-0": { "": "src/" },
        "psr-4": {
            "Supabase\\Storage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Supabase\\Storage\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "jordan",
            "email": "jordan@ascensionengineer.com"
        },
        {
            "name": "norm",
            "email": "norm@zerocopylabs.com"
        },
        {
            "name": "Adolfo",
            "email": "adolfo@zerocopylabs.com"
        },
        {
            "name": "Luis",
            "email": "luis@zerocopylabs.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "9.6",
        "squizlabs/php_codesniffer": "3.*",
        "allure-framework/allure-phpunit": "1.5",
        "mockery/mockery": "^1.5"
    },
    "require": {
        "guzzlehttp/guzzle": "7.5",
        "vlucas/phpdotenv": "5.5",
        "bayfrontmedia/php-mime-types": "^2.0",
        "league/uri": "^6.0"
    }
}
